@prefix dlzkr: <http://vito.be/voc/dlzkr#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: 		<http://xmlns.com/foaf/0.1/> .

<http://vito.be/voc/dlzkr> rdf:type owl:Ontology ;
    dc:title "Doelzoeker ontology" ; 
    dc:description "Draft Doelzoeker ontology"; 
    owl:versionInfo "0.1" . 

dlzkr:Doelzoeker a owl:Class ; 
    rdfs:label "Doelzoeker" ; 
    dlzkr:name "Doelzoeker" ;
    dlzkr:contains dlzkr:Goal ;
    dlzkr:definedBy dlzkr:User .

dlzkr:Goal rdf:type owl:Class ;
    rdfs:label "Goal" ;
    dlzkr:name "Goal".

dlzkr:satisfiedWithProgress rdf:type owl:DatatypeProperty ;
                       rdfs:label "satisfiedWithProgress" ; 
                       rdfs:domain dlzkr:Goal ;
                       rdfs:range xsd:boolean . 

dlzkr:contains rdf:type owl:ObjectProperty ;
          rdfs:label "contains"; 
          rdfs:domain dlzkr:Doelzoeker;
          rdfs:range dlzkr:Goal . 

dlzkr:User a owl:Class ; 
      rdfs:subClassOf foaf:Person ;
      rdfs:label "User" . 

dlzkr:TimeWindow a owl:Class ;
    owl:oneOf (:verleden :heden :toekomst) .

dlzkr:definedBy rdf:type owl:ObjectProperty ;
    rdfs:label "definedBy"; 
    rdfs:domain dlzkr:Doelzoeker;
    rdfs:range dlzkr:User . 